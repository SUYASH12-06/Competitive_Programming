#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
     int t;
     cin>>t;
     while(t!=0){
         int a,b,c,x;
         cin>>a>>b>>c>>x;
         if(a+b>=x||b+c>=x||a+c>=x){
             cout<<"YES"<<endl;
         }else{
             cout<<"NO"<<endl;
         }
         t--;
     }
}
