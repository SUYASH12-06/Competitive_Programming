#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;cin>>t;
	while(t>0){
	    int n,x;
	    cin>>n>>x;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    int cnt=0;
	    for(int i=0;i<n;i++){
	        if(arr[i]>=x){
	            cnt++;
	        }
	    }
	    
	    cout<<cnt<<endl;
	    
	    
	    t--;
	}
	

}
