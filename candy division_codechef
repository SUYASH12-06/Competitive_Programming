#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
      int t; //t=no of test cases
      cin>>t;
      while(t>0){
          int n;
          cin>>n; //n=no of candies
          if(n<=0){
              cout<<"No"<<endl;
          }
          else if(n%3==0){
          cout<<"Yes"<<endl;
          }
          else{
          cout<<"No"<<endl;
          }
          t--;
      }
      
}
