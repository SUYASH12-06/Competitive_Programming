#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t; //t=no of testcases
	while(t>0){
	    int n;   //n= no of chocolates
	    cin>>n;
	    if(n==0)   //edge condn
        cout<<"No"<<endl;
	    if(n%2==0){
	        cout<<"Yes"<<endl;
	    }else{
	        cout<<"No"<<endl;
	    }
	    t--;
	}

}
